{
  "name": "p5-calc",
  "version": "0.1.0",
  "description": "Persona 5 Royal Calculator",
  "author": "Silent",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.tools.json webpack --config configs/webpack/webpack.prod.ts",
    "build:analyze": "cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.tools.json webpack -p --config configs/webpack/webpack.prod.analyze.ts",
    "clean": "rimraf build",
    "clean:all": "yarn clean && rimraf node_modules yarn.lock",
    "dev": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.tools.json webpack-dev-server --config configs/webpack/webpack.dev.ts",
    "lint": "cross-env NODE_ENV=development eslint . --cache --ext .js,.ts,.tsx",
    "lint:fix": "yarn --silent lint --fix",
    "postlint:fix": "cross-env prettier --ignore-path .eslintignore --write '**/*.{js,ts,tsx,json,html,css,less,scss,yml}'"
  },
  "dependencies": {
    "@fontsource/fira-code": "^4.5.1",
    "@fortawesome/fontawesome-svg-core": "1.3.0-beta2",
    "@fortawesome/free-solid-svg-icons": "6.0.0-beta2",
    "@fortawesome/react-fontawesome": "0.1.16",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bootstrap": "5.1.3",
    "css-loader": "6.5.1",
    "date-fns": "^2.25.0",
    "react": "17.0.2",
    "react-bootstrap": "2.0.2",
    "react-dom": "17.0.2",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.3.0",
    "react-table": "7.7.0",
    "react-table-sticky": "1.1.3",
    "sass": "1.43.4",
    "sass-loader": "12.3.0",
    "style-loader": "3.3.1",
    "url-loader": "4.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/plugin-proposal-class-properties": "7.16.0",
    "@babel/plugin-proposal-decorators": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.0",
    "@babel/preset-env": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@babel/runtime": "7.16.3",
    "@types/html-webpack-plugin": "3.2.6",
    "@types/node": "16.11.7",
    "@types/react": "17.0.34",
    "@types/react-dom": "17.0.11",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-table": "7.7.8",
    "@types/webpack": "5.28.0",
    "@types/webpack-bundle-analyzer": "4.4.1",
    "@types/webpack-dev-server": "4.3.1",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "babel-loader": "8.2.3",
    "cross-env": "7.0.3",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.0",
    "fork-ts-checker-webpack-plugin": "6.4.0",
    "html-webpack-plugin": "5.5.0",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "ts-loader": "9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.4.4",
    "webpack": "5.63.0",
    "webpack-bundle-analyzer": "4.5.0",
